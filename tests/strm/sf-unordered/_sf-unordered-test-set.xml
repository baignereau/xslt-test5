          xmlns="http://www.w3.org/2012/10/xslt-test-catalog"
          name="sf-unordered">
   <description>unordered() function with streaming</description>
   
   <environment name="sf-unordered-A">
     <stylesheet file="sf-unordered-A.xsl"/>
