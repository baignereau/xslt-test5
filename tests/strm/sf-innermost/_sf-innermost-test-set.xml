<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.w3.org/2012/10/xslt-test-catalog"
          name="sf-innermost">
   <description>innermost() function with streaming</description>
   
   <environment name="sf-innermost-A">
     <stylesheet file="sf-innermost-A.xsl"/>
   </environment>
   
   <dependencies>
      <spec value="XSLT30+"/>
      <feature value="streaming"/>
   </dependencies>
   
   <test-case name="sf-innermost-001">
      <description>Streaming innermost() - grounded operand</description>
      <created by="Michael Kay" on="2013-11-25"/>
      <environment ref="sf-innermost-A"/>
      <test>
         <initial-template name="r-001"/>
      </test>
      <result>
         <assert-xml><![CDATA[<out>4.95 4.95 4.95 16.47 16.47</out>]]></assert-xml>
      </result>
   </test-case>
   
