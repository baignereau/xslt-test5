   <test-case name="unicode90-001">
      <description>Tests the new digits in Unicode 9.0</description>
      <created by="Abel Braaksma" on="2016-09-08"/>
      <test>
         <stylesheet file="unicode90-001.xsl"/>
         <param name="regex" static="yes" select="'\d'" />
      </test>
      <result>
         <assert>/count = 370</assert>
      </result>
   </test-case>
   
