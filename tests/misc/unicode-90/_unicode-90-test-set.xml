<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns="http://www.w3.org/2012/10/xslt-test-catalog" name="unicode-90">
   <description>Tests whitespace</description>
   
   <dependencies>
      <spec value="XSLT30+" />
      <unicode-version value="9.0" satisfied="true"/>
   </dependencies>
   
   <test-case name="unicode90-001">
      <description>Tests the new decimals in Unicode 9.0</description>
      <created by="Abel Braaksma" on="2016-09-08"/>
      <test>
         <stylesheet file="unicode90-001.xsl"/>
         <param name="regex" static="yes" select="'\d'" />
      </test>
      <result>
         <assert>/count = 370</assert>
      </result>
   </test-case>
   
   <test-case name="unicode90-002">
      <description>Tests the new non-decimals in Unicode 9.0</description>
      <created by="Abel Braaksma" on="2016-09-08"/>
      <test>
         <stylesheet file="unicode90-001.xsl"/>
         <param name="regex" static="yes" select="'\D'" />
      </test>
      <result>
         <assert>/count = 63084</assert>
      </result>
   </test-case>
      
