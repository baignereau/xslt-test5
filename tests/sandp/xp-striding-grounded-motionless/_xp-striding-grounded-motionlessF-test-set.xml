<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns="http://www.w3.org/2012/10/xslt-test-catalog" name="xp-striding-grounded-motionlessF">
   <description>
                    CP: Striding, RP: Grounded, RS: Motionless
                    Self axis on parent or ancestor axis which is not on U{element, document}} selects nothingness
                </description>
   <test-case name="xp-striding-grounded-motionlessF001">
      <description>
                CP: striding, RP: grounded, RS: motionless
                Base expression: parent::foo
                Separators:      / 
                With axes:       self
                With node types: attribute, namespace, comment, pi, text
                Invariant:       none
            </description>
      <created by="Abel Braaksma" on="2014-10-15"/>
      <test>
         <posture-and-sweep context-posture="striding">
            <xpath>parent::foo/self::attribute()</xpath>
            <xpath>parent::foo/self::attribute(*)</xpath>
            <xpath>parent::foo/self::attribute(ex:foo)</xpath>
            <xpath>parent::foo/self::attribute(Q{http://example.com}foo)</xpath>
            <xpath>parent::foo/self::comment()</xpath>
            <xpath>parent::foo/self::processing-instruction()</xpath>
            <xpath>parent::foo/self::processing-instruction(xml-stylesheet)</xpath>
            <xpath>parent::foo/self::processing-instruction('xml-stylesheet')</xpath>
            <xpath>parent::foo/self::text()</xpath>
            <xpath>parent::foo/self::namespace-node()</xpath>
         </posture-and-sweep>
      </test>
      <result>
         <assert-posture-and-sweep result-posture="grounded" result-sweep="motionless"/>
      </result>
   </test-case>
