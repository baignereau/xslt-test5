   <description>
                    CP: Striding, RP: Grounded, RS: Motionless
                    Self-axis that changes axes selects nothing, so grounded/motionless
                </description>
   <test-case name="xp-striding-grounded-motionlessC001">
      <description>
                CP: striding, RP: grounded, RS: motionless
                Base expression: self::foo
                Separators:      / 
                With axes:       self
                With node types: any
                Invariant:       node-type-unequal
            </description>
      <created by="Abel Braaksma" on="2014-10-16"/>
      <test>
         <posture-and-sweep context-posture="striding">
            <xpath>self::foo/self::document-node()</xpath>
            <xpath>self::foo/self::attribute()</xpath>
            <xpath>self::foo/self::attribute(*)</xpath>
            <xpath>self::foo/self::attribute(ex:foo)</xpath>
            <xpath>self::foo/self::attribute(Q{http://example.com}foo)</xpath>
            <xpath>self::foo/self::comment()</xpath>
            <xpath>self::foo/self::processing-instruction()</xpath>
            <xpath>self::foo/self::processing-instruction(xml-stylesheet)</xpath>
            <xpath>self::foo/self::processing-instruction('xml-stylesheet')</xpath>
            <xpath>self::foo/self::text()</xpath>
            <xpath>self::foo/self::namespace-node()</xpath>
         </posture-and-sweep>
      </test>
      <result>
         <assert-posture-and-sweep result-posture="grounded" result-sweep="motionless"/>
      </result>
   </test-case>
