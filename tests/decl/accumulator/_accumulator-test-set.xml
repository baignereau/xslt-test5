         <stylesheet file="accumulator-007.xsl"/>
         <param name="streamable" static="yes" select="'no'"/>
      </test>
      <result>
         <assert-xml><![CDATA[<result min="-16" max="182" sum="277.43" count="16" avg="17.34"/>]]></assert-xml>
