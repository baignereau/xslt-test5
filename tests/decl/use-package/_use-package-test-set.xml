         <initial-template name="main"/>
      </test>
      <result>
         <all-of>
            <assert>/out/@p = 'one'</assert>
            <assert>/out/@q = 'three'</assert>
         </all-of>
      </result>
   </test-case>

   <test-case name="use-package-103">
      <description>Namespace aliases are local to a package</description>
